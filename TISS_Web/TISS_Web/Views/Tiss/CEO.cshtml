    <script>

    $(document).ready(function ()
    {
        var isLoggedIn = '@Session["LoggedIn"]' == 'True';
        if (isLoggedIn) {
            $('#ceo').attr('contenteditable', 'true');
            $('#logoutBtn').show(); //顯示登出按鈕
        } else {
            $('#ceo').attr('contenteditable', 'false');
            $('#loginBtn').show(); //顯示登入按鈕
        }

        function toggleDropdown(dropdownId) {
            var dropdownToggle = document.getElementById(dropdownId);
            var dropdownMenu = dropdownToggle.nextElementSibling;

            if (dropdownMenu.classList.contains('show')) {
                dropdownMenu.classList.remove('show');
            } else {
                dropdownMenu.classList.add('show');
            }
        }
    });
        //圖片及文字存檔功能
        function saveContent()
        {
            var textContent = document.getElementById('ceo').innerHTML; // 修改為 innerHTML

            $.ajax({
                url: 'CeoSaveContent',
                method: 'POST',
                data: {
                    TextContent: textContent,
                },
                success: function (response) {
                    alert('內容已儲存');
                },
                error: function (err) {
                    console.error('儲存失敗', err);
                }
            });
        }

        //載入已儲存的內容
        function loadContent()
        {
            $.ajax(
                {
                    url: 'CeoGetContent',
                    method: 'GET',
                    success: function (response)
                    {
                        if (response.success)
                        {
                            document.getElementById('ceo').innerHTML = response.textContent; // 修改為 innerHTML
                        }
                        else
                        {
                            console.error('載入失敗: ' + response.error);
                        }
                    },
                    error: function (err)
                    {
                        console.error('載入失敗', err);
                    }
                });
        }
        loadContent(); // 修正位置：確保這行程式碼在頁面完全加載後執行

    </script>
